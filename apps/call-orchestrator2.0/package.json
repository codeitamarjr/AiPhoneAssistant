{
  "name": "call-orchestrator2.0",
  "version": "1.0.1",
  "private": true,
  "description": "A Fastify-based application for handling calls and interactions with OpenAI API",
  "type": "module",
  "engines": { "node": ">=18.17" },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node --enable-source-maps --loader tsx src/index.ts",
    "build": "tsc -p .",
    "tunnel": "ngrok http 5050 --log=stdout",
    "dev:tunneled": "concurrently -k -s first -n server,ngrok -c blue,magenta \"npm:dev\" \"npm:tunnel\""
  },
  "author": "Itamar Junior",
  "license": "ISC",
  "dependencies": {
    "@fastify/formbody": "^8.0.0",
    "@fastify/websocket": "^11.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "concurrently": "^9.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
